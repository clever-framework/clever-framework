<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="io.github.toquery.framework.system.mapper.SysUserMapper">

    <resultMap id="roleAndMenuResultMap" type="io.github.toquery.framework.system.entity.SysUser">
        <id column="id" property="id"/>
        <result column="username" property="username"/>
    </resultMap>
    <select id="getByUsername" resultType="io.github.toquery.framework.system.entity.SysUser">
        select *
        from sys_user
        where username = #{username}
    </select>


</mapper>
